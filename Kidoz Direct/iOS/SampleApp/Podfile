workspace 'SampleApp.xcworkspace'
platform :ios, '12.0'

def kidoz_pod
    #pod 'KidozSDK','9.1.5'
    pod ‘KidozSDK’, :podspec => 'https://github.com/Kidoz-SDK/kidoz-ios-frameworks/raw/refs/heads/beta/KidozSDK/10.0.0/KidozSDK.podspec'
end

target 'SampleAppSwift' do
  use_frameworks!
  kidoz_pod
  project 'SampleAppSwift/SampleAppSwift.xcodeproj'
end

target 'SampleAppObjc' do
  use_frameworks!
  kidoz_pod
  project 'SampleAppObjc/SampleAppObjc.xcodeproj'
end

target 'CommonSampleApp' do
  use_frameworks!
  kidoz_pod
  project 'CommonSampleApp/CommonSampleApp.xcodeproj'
end
